
<%# <%= link_to "dungeons", dungeons_path(@dungeons), class: "btn-btn-flat" %>
<%= simple_form_for @dungeon do |f| %>

  <%= f.input :name %>

  <%= f.input :address %>

  <%= f.input :price %>

  <%= f.input :description %>

  <%= f.input :guests %>

  <%= f.input :photo, as: :file %>

  <%= f.input :dragon %>

  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
